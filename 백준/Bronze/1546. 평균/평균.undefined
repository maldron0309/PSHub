#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main(){
    int n;
    vector<int> scores;
    cin >> n;

    for (int i = 0; i < n; i++) {
        int x;
        cin >> x;
        scores.push_back(x);
    }

    int m = *max_element(scores.begin(), scores.end());
    double sum = 0;
    for (int s : scores) {
        sum += (double)s / m * 100;
    }
    cout << sum / n;
}
