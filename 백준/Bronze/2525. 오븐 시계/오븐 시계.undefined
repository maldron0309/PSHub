#include <iostream>
using namespace std;

int main()
{
	int hours, minutes;
	cin >> hours >> minutes;

	int cook;
	cin >> cook;

	int total = hours * 60 + minutes + cook;
	int endHours = total / 60 % 24;
	int endMinutes = total % 60;

	cout << endHours << ' ' << endMinutes;


	return 0;
}