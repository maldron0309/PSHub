#include <iostream>
#include <string>
using namespace std;

int main() {
    int t;
    cin >> t;

    while (t--) {
        string s;
        cin >> s;

        int cnt = 0;
        for (char c : s) {
            if (c == '(') cnt++;
            else cnt--;
            if (cnt < 0) break;
        }

        if (cnt == 0) cout << "YES\n";
        else cout << "NO\n";
    }
}
